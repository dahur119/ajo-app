{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testRegex": "src/e2e/.*\\.e2e\\.spec\\.ts$",
  "setupFilesAfterEnv": ["./test/jest-e2e.setup.ts"],
  "collectCoverageFrom": ["src/**/*.(t|j)s"],
  "coverageDirectory": "../coverage"
}